{
	"name": "baraja-core/doctrine-mail-message",
	"description": "Converter Mail message to Doctrine entity.",
	"homepage": "https://github.com/baraja-core/doctrine-mail-message",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"nette/di": "^3.0",
		"nette/mail": "^3.0",
		"nette/utils": "^3.0",
		"baraja-core/doctrine": "~2.0"
	},
	"require-dev": {
		"symplify/easy-coding-standard": "^7.2",
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 6 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
