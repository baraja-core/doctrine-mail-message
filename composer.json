{
	"name": "baraja-core/doctrine-mail-message",
	"description": "Converter Mail message to Doctrine entity.",
	"homepage": "https://github.com/baraja-core/doctrine-mail-message",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": "^8.0",
		"nette/di": "^3.0",
		"nette/mail": "^3.0",
		"nette/utils": "^3.0",
		"doctrine/orm": "^2.7",
		"doctrine/dbal": "^2.9"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.74",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.14",
		"roave/security-advisories": "dev-master",
		"baraja-core/doctrine": "^3.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
